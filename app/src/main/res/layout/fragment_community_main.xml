                android:paddingTop="9.51dp"
                android:paddingBottom="7.56dp"
                android:paddingEnd="8.77dp"
                android:paddingStart="10.21dp"
                android:layout_marginBottom="25dp"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginEnd="14.65dp"
                    android:layout_gravity="center"
                    android:src="@drawable/communitymain_pink_number_2"/>
