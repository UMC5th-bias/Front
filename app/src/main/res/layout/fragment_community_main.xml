                android:paddingTop="9.51dp"
                android:paddingBottom="7.56dp"
                android:paddingEnd="8.77dp"
                android:paddingStart="10.21dp"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginEnd="14.65dp"
                    android:layout_gravity="center"
                    android:src="@drawable/communitymain_black_number_1"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:text="나고야 주변 성지순례 리스트 모음집.zip"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/comminitymain_article_background"
