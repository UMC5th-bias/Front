                android:layout_height="wrap_content"
                android:text="자유게시판"
                android:textSize="12sp"
                android:textColor="#F73D93"
                android:layout_marginTop="25dp"
                android:layout_marginStart="15.59dp"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="성지순례인증"
                android:textSize="12sp"
                android:textColor="#CFCFCF"
                android:layout_marginTop="25dp"
                android:layout_marginStart="17.47dp"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_marginHorizontal="15dp"
            android:paddingVertical="13.19dp"
            android:paddingHorizontal="18.8dp"
            android:background="@drawable/communitymain_articlelist_background"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
